<view class="container"><view class="header"><text class="greeting">{{a}}</text><text class="date">{{b}}</text></view><view class="tree-section"><view class="tree-container"><view class="{{['tree', e, f]}}"><view class="tree-trunk"></view><view class="tree-leaves tree-leaves-1"></view><view class="tree-leaves tree-leaves-2"></view><view class="tree-leaves tree-leaves-3"></view><view class="tree-decorations"><view wx:if="{{c}}" class="flower"></view><view wx:if="{{d}}" class="fruit"></view></view></view><view class="tree-ground"></view></view><view class="level-info"><text class="level-text">成长等级 {{g}}</text><view class="exp-bar"><view class="exp-progress" style="{{'width:' + h}}"></view></view><text class="exp-text">{{i}}/{{j}} 分钟</text></view></view><view class="overview-card"><view class="card-header"><image class="card-icon" src="{{k}}" mode="aspectFit"></image><text class="card-title">今日概览</text></view><view class="stats"><view class="stat-item"><image class="stat-icon" src="{{l}}" mode="aspectFit"></image><text class="number">{{m}}</text><text class="label">已完成</text></view><view class="stat-item"><image class="stat-icon" src="{{n}}" mode="aspectFit"></image><text class="number">{{o}}</text><text class="label">专注时间</text></view></view></view><view class="quick-actions"><view class="action-item" bindtap="{{q}}"><uni-icons wx:if="{{p}}" u-i="7dc9037c-0" bind:__l="__l" u-p="{{p}}"></uni-icons><text>开始专注</text></view><view class="action-item" bindtap="{{s}}"><uni-icons wx:if="{{r}}" u-i="7dc9037c-1" bind:__l="__l" u-p="{{r}}"></uni-icons><text>添加任务</text></view><view class="action-item" bindtap="{{v}}"><uni-icons wx:if="{{t}}" u-i="7dc9037c-2" bind:__l="__l" u-p="{{t}}"></uni-icons><text>数据统计</text></view></view><view class="todo-section"><view class="card-header"><image class="card-icon" src="{{w}}" mode="aspectFit"></image><text>今日待办</text><text class="more" bindtap="{{x}}">查看全部</text></view><view class="todo-list"><block wx:if="{{y}}"><view wx:for="{{z}}" wx:for-item="item" wx:key="e" class="todo-item"><checkbox checked="{{item.a}}" bindtap="{{item.b}}"/><text class="{{[item.d && 'completed']}}">{{item.c}}</text></view></block><view wx:else class="empty-tasks"><image class="empty-image" src="{{A}}" mode="aspectFit"></image><text>还没有待办任务哦</text></view></view></view></view>